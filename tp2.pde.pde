int A;

void setup(){
size(400,400);
A = -5;
}

void draw(){

translate( width/2, height/2);
rotate(radians(-frameCount*2));
rectMode(CENTER);
rotate(radians (A));
rect(0,0,300,300);

funcion (2);

funcion(5);



rectMode(CENTER);
rotate(radians (A));
rect(0,0,200,200);
fill(200);

if (key == ' '){A += 5;}
else if (key=='s'){A-= 1;}

}
